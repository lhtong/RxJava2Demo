<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.usm.rxjava2demo.OperatorActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="observableClickMap"
            android:text="Map操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="observableClickFlatMap"
            android:text="flatMap操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="observableClickConcatMap"
            android:text="concatMap操作符" />


        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="observableClickZip"
            android:text="zip操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="concat"
            android:text="concat操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="distinct"
            android:text="distinct操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="filter"
            android:text="filter操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="buffer"
            android:text="buffer操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="timer"
            android:text="timer操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="interval"
            android:text="interval操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="doOnNext"
            android:text="doOnNext操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="skip"
            android:text="skip操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="take"
            android:text="take操作符" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="debounce"
            android:text="debounce操作符"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="last"
            android:text="last操作符"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="merge"
            android:text="merge操作符"
            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="reduce"
            android:text="reduce操作符"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="scan"
            android:text="scan操作符"
            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="window"
            android:text="window操作符"
            />

    </LinearLayout>
</ScrollView>
